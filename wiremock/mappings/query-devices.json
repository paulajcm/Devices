{
  "request": {
    "method": "GET",
    "urlPath": "/v1/devices/query",
    "queryParameters" : {
      "name": {
        "matches" : ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "fixedDelayMilliseconds": 1000,
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Content-Type": "application/json"
    },
    "bodyFileName": "devices-query.json"
  }
}